script(src='https://code.jquery.com/jquery-3.4.1.js', integrity='sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU=', crossorigin='anonymous')
        

body
  Head Client Side Test
  div
    button(onclick='clickEvnet()') toggle Btn..
  div
    button(id="ajaxbnt" onclick='dataFromServer()') Get Data ..
  


  div(id="showData")

script.
  var toggle = true;
  const clickEvnet = function() {
    toggle ? $("#ajaxbnt").hide() : $("#ajaxbnt").show();
    toggle = !toggle;

    console.log('clickEvnet')
    //alert('clickEvnet')
  }
  const dataFromServer = function(){
    $.ajax({
            url: "/list",
            success: function(data){
                console.log('data : ',data)
                const htmlwithData = `<div>${data.listData} </div>`

                $("#showData").html(htmlwithData) ;
            }
        });

  }
